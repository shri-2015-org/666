{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-var": [1],
    "new-cap": [0],
    "no-console": [0],
    "dot-notation": [1],
    "no-unused-vars": [1],
    "react/prop-types": [1],
    "indent": [2, 2, {"SwitchCase": 1}]
  },
  "globals": {
    "window": true,
    "document": true,
  }

}
