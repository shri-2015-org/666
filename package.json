{
  "name": "anonym.club",
  "version": "0.0.1",
  "description": "anonym.club",
  "scripts": {
    "lint": "eslint .",
    "dev": "babel-node server/server.dev.js",
    "prod": "npm run build; cd build; npm run start",
    "build": "npm run lint; npm run test; bash build.sh",
    "test": "npm run test-server; npm run test-common; npm run test-client",
    "test-server": "mocha --compilers js:babel/register 'server/**/*.test.js'",
    "test-common": "mocha --compilers js:babel/register 'common/**/*.test.js'",
    "test-client": "mocha --compilers js:babel/register 'client/**/*.test.js'",
    "autotest": "supervisor -q -n exit -x mocha -- -b  --compilers js:babel/register 'server/actions.test.js'"
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-plugin-node-env-inline": "1.0.1",
    "debug": "2.2.0",
    "express": "4.13.3",
    "lodash": "3.10.1",
    "react": "0.13.3",
    "socket.io": "1.3.6",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel-core": "5.8.25",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "chai": "3.2.0",
    "css-loader": "0.18.0",
    "eslint": "1.4.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "3.3.2",
    "extract-text-webpack-plugin": "0.8.2",
    "html-webpack-plugin": "1.6.1",
    "mocha": "2.3.2",
    "node-sass": "3.3.3",
    "pre-commit": "1.1.1",
    "react-hot-loader": "1.3.0",
    "react-redux": "2.1.2",
    "redux": "3.0.0",
    "redux-logger": "1.0.8",
    "sass-loader": "2.0.1",
    "socket.io-client": "1.3.6",
    "style-loader": "0.12.4",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.11.0"
  },
  "author": "anonym club star team",
  "pre-commit": [
    "lint",
    "test"
  ]
}
